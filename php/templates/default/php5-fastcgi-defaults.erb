START=yes

# Which user runs PHP? (default: www-data)

EXEC_AS_USER=<%= @node[:php][:user] %>

# Host and TCP port for FASTCGI-Listener (default: localhost:9000)

FCGI_HOST=<%= @node[:php][:host] %>
FCGI_PORT=<%= @node[:php][:port] %>

# Environment variables, which are processed by PHP

PHP_FCGI_CHILDREN=<%= @node[:php][:children] %>
PHP_FCGI_MAX_REQUESTS=<%= @node[:php][:max_requests] %>